package trabalho;

public class Veiculo {
	private int codigo;
	private String marca;
	private String modelo;
	private int anoFabricacao;
	private int idLoja;
	
	public Veiculo(int codigo, String marca, String modelo, int anoFabricacao, int idLoja) {
		this.codigo = codigo;
		this.marca = marca;
		this.modelo = modelo;
		this.anoFabricacao = anoFabricacao;
		this.idLoja = idLoja;
	}
	
	@Override
	 public String toString() {
	     return "CÃ³digo: " + codigo + "\n" +
	            "Marca: " + marca + "\n" +
	            "Modelo: " + modelo + "\n" +
	            "Ano: " + anoFabricacao + "\n" +
	            "Id da Loja: " + idLoja;
	            
	 }
	    
	public int getCodigo() {
		return codigo;
	}
	public void setCodigo(int codigo) {
		this.codigo = codigo;
	}
	public String getMarca() {
		return marca;
	}
	public void setMarca(String marca) {
		this.marca = marca;
	}
	public String getModelo() {
		return modelo;
	}
	public void setModelo(String modelo) {
		this.modelo = modelo;
	}
	public int getAnoFabricacao() {
		return anoFabricacao;
	}
	public void setAnoFabricacao(int anoFabricacao) {
		this.anoFabricacao = anoFabricacao;
	}
	public int getLoja() {
		return idLoja;
	}
	public void setLoja(int idLoja) {
		this.idLoja = idLoja;
	}
	
}
